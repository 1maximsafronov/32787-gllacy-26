{
  "name": "gllacy-26",
  "version": "1.0.0",
  "description": "Личный проект «Глейси» от HTML Academy",
  "private": true,
  "scripts": {
    "start": "gulp start",
    "build": "gulp build",
    "publish": "gulp publish",
    "test": "npm run editorconfig && npm run stylelint",
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"src/sass/**/*.scss\" --syntax scss"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "editorconfig-cli": [
    "*.js",
    "*.json",
    "src/*.html",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "^10.3.1",
    "browser-sync": "^2.27.5",
    "del": "^6.0.0",
    "gh-pages": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-libsquoosh": "^1.0.9",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgstore": "^8.0.0",
    "gulp-terser": "^2.0.1",
    "gulp-webp": "^4.0.1",
    "normalize.css": "^8.0.1",
    "postcss": "^8.2.8",
    "postcss-csso": "^5.0.1",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.10"
  },
  "engines": {
    "node": "^12 || ^14"
  }
}
